{
	"name": "directus-extension-easycloud",
	"version": "1.0.0",
	"type": "module",
	"directus:extension": {
		"host": "^10.1.9",
		"type": "bundle",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"entries": [
			{
				"type": "hook",
				"name": "socketio-server",
				"source": "src/socketio-server/index.ts"
			},
			{
				"type": "endpoint",
				"name": "socketio-server-api",
				"source": "src/socketio-server-api/index.ts"
			},
			{
				"type": "endpoint",
				"name": "demo1",
				"source": "src/demo1/index.ts"
			},
			{
				"type": "hook",
				"name": "serve-app",
				"source": "src/serve-app/index.ts"
			},
			{
				"type": "endpoint",
				"name": "wechat-proxy",
				"source": "src/wechat-proxy/index.ts"
			},
			{
				"type": "endpoint",
				"name": "otp-auth",
				"source": "src/otp-auth/index.ts"
			},
			{
				"type": "hook",
				"name": "otp-auth-hook",
				"source": "src/otp-auth-hook/index.ts"
			}
		]
	},
	"description": "Please enter a description for your extension",
	"icon": "extension",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-bundle"
	],
	"scripts": {
		"build": "directus-extension build --no-minify --sourcemap",
		"dev": "directus-extension build -w --no-minify --sourcemap",
		"link": "directus-extension link",
		"add": "directus-extension add"
	},
	"dependencies": {
		"@directus/api": "workspace:*",
		"@directus/env": "workspace:*",
		"@directus/errors": "workspace:*",
		"socket.io": "4.8.1",
		"formidable": "3.5.4",
		"express": "catalog:",
		"nanoid": "catalog:",
		"rate-limiter-flexible": "catalog:"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "workspace:*",
		"@directus/types": "workspace:*",
		"@types/node": "catalog:",
		"@types/express": "catalog:",
		"@types/formidable": "3.4.6",
		"typescript": "catalog:"
	}
}
