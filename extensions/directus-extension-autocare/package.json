{
	"name": "directus-extension-autocare",
	"description": "Please enter a description for your extension",
	"icon": "extension",
	"version": "1.0.0",
	"license": "UNLICENSED",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-bundle"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "bundle",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"entries": [
			{
				"type": "endpoint",
				"name": "autocare-api",
				"source": "src/autocare-api/index.ts"
			},
			{
				"type": "hook",
				"name": "autocare-hook",
				"source": "src/autocare-hook/index.ts"
			}
		],
		"host": "^10.10.0"
	},
	"scripts": {
		"deploy": "directus-extension build --no-minify && bash deploy.sh",
		"build": "directus-extension build --no-minify",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"validate": "directus-extension validate",
		"add": "directus-extension add",
		"test": "vitest run"
	},
	"dependencies": {
		"@directus/api": "workspace:*",
		"@directus/errors": "workspace:*",
		"@directus/utils": "workspace:*",
		"express": "catalog:",
		"knex": "catalog:",
		"date-fns": "catalog:",
		"pino": "catalog:"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "workspace:*",
		"@directus/types": "workspace:*",
		"@types/node": "catalog:",
		"@types/express": "catalog:",
		"vitest": "catalog:",
		"typescript": "catalog:"
	}
}
